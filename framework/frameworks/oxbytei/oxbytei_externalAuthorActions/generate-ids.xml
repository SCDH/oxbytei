<?xml version="1.0" encoding="UTF-8"?>
<!-- user action -->
<!-- This is just there in order to have a button for the "Generate IDs" action from the TEI P5 framework. -->
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="oxbytei.generate.ids">
  <a:name>Generate IDs</a:name>
  <a:description>Generate IDs (TEI P5).</a:description>
  <a:smallIconPath href="${framework(oXbytei)}/images/IDs-16.png"/>
  <a:largeIconPath href="${framework(oXbytei)}/images/IDs-24.png"/>
  <a:operations>
    <a:operation id="ExecuteMultipleActionsOperation">
      <a:xpathCondition>true()</a:xpathCondition>
      <a:arguments>
        <a:argument name="actionIDs">select.for.ids, generate.ids</a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:accessKey/>
</a:authorAction>
